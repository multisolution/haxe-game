<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="HaxeGame" package="br.art.multisolution.haxegame" version="0.0.0" company="Multisolution" />
	<app main="Main" path="Export" file="HaxeGame" />
	
	<source path="Source" />
	
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<haxelib name="nape" />
	
	<assets path="Assets" rename="assets" />
	<assets path="Assets/fonts" rename="fonts" />
	
	<window
		width="360" height="640"
		orientation="portrait"
		background="#FFFFFF"
		fps="60"
		hardware="true"
		fullscreen="true"
		resizable="true"
		borderless="true"
		vsync="true"
	/>
	
</project>